<?xml version="1.0"?>
<!DOCTYPE look-and-feel PUBLIC "-//Liferay//DTD Look and Feel 6.2.0//EN" "http://www.liferay.com/dtd/liferay-look-and-feel_6_2_0.dtd">

<look-and-feel>
	<compatibility>
		<version>6.2.0+</version>
	</compatibility>
	<theme id="rev-Theme" name="Tema de intranet">
		<settings>
			<setting configurable="true" key="dockbar-mode" type="select" options="normal,split" value="normal"></setting>
			<setting configurable="true" key="portlet-setup-show-borders-default" type="checkbox" value="false"></setting>
			<setting configurable="true" key="breadcrumbs" type="checkbox" value="true"></setting>
			<setting configurable="true" key="navigation" type="select" options="left,right" value="right"></setting>
			<setting configurable="true" key="logo-menu-direction" type="select" options="left-right,right-left" value="left-right"></setting>
			<setting configurable="true" key="footer-web-content-article-id" type="text" value=""></setting>
		</settings>
	</theme>
</look-and-feel>